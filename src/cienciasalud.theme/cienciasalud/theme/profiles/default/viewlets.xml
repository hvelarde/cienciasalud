<?xml version="1.0"?>
<object>

  <!-- Portal top -->
  <order manager="plone.portalheader" skinname="Ciencia y Salud" based-on="Plone Default">
    <viewlet name="cienciasalud.ad" insert-before="plone.logo" />
  </order>

  <hidden manager="plone.portalheader" skinname="Ciencia y Salud">
    <viewlet name="plone.searchbox" />
  </hidden>

  <hidden manager="plone.portalheader" skinname="Ciencia y Salud">
    <viewlet name="plone.personal_bar" />
  </hidden>

  <!-- Above content -->
  <order manager="plone.abovecontent" skinname="Ciencia y Salud" based-on="Plone Default">
    <viewlet name="cienciasalud.news" />
    <viewlet name="plone.path_bar" />
  </order>

  <!-- Portal footer -->
  <order manager="plone.portalfooter" skinname="Ciencia y Salud" based-on="Plone Default">
    <viewlet name="plone.personal_bar" insert-after="*" />
  </order>

</object>
