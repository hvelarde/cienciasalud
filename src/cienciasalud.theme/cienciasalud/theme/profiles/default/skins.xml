<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="Ciencia y Salud">

 <object name="cys_theme_custom_images"
    meta_type="Filesystem Directory View"
    directory="cys.theme:skins/cys_theme_custom_images"/>
 <object name="cys_theme_custom_templates"
    meta_type="Filesystem Directory View"
    directory="cys.theme:skins/cys_theme_custom_templates"/>
 <object name="cys_theme_styles"
    meta_type="Filesystem Directory View"
    directory="cys.theme:skins/cys_theme_styles"/>

 <skin-path name="Ciencia y Salud" based-on="Sunburst Theme">
  <layer name="cys_theme_custom_images"
     insert-after="custom"/>
  <layer name="cys_theme_custom_templates"
     insert-after="cys_theme_custom_images"/>
  <layer name="cys_theme_styles"
     insert-after="cys_theme_custom_templates"/>
 </skin-path>

</object>
